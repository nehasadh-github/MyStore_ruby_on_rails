<div class="shopper">
  <div id='columns'>
    <div id='side' >

      <div id='cart'>


      </div>
    </div>
    <div id='main' >
      <%= notice %>
      <br><h3><a href = "http://localhost:3000/">Home</a></h3>
      <%= form_with(model: @admin) do |form| %>
      <div class="field">
        <h3>
        <%= form.label :UserId %>
        <%= form.text_field :userid %>
        </h3>
      </div>

      <div class="field">
        <h3>
        <%= form.label :Password %>
        <%= form.password_field :password %>
        </h3>
      </div>

      <div class="field">
        <%= form.submit :Login %>
      </div>
      <% end %>




    </div>

  </div>
</div>
